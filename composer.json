{
  "name": "webdevstudios/wp-search-with-algolia",
  "description": "Integrate the powerful Algolia search service with WordPress.",
  "keywords": [
    "wordpress"
  ],
  "homepage": "https://github.com/WebDevStudios/wp-search-with-algolia",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "WebDevStudios",
      "email": "contact@webdevstudios.com",
      "homepage": "https://github.com/WebDevStudios/wp-search-with-algolia"
    }
  ],
  "type": "wordpress-plugin",
  "support": {
    "issues": "https://github.com/WebDevStudios/wp-search-with-algolia/issues",
    "forum": "https://wordpress.org/support/plugin/wp-search-with-algolia"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "includes/"
    ]
  },
  "require": {
    "php": ">=5.6",
    "algolia/algoliasearch-client-php": "^2.4"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "phpcompatibility/php-compatibility": "^9.2.0",
    "phpcompatibility/phpcompatibility-paragonie": "^1.0.0",
    "phpcompatibility/phpcompatibility-wp": "^2.0.0",
    "squizlabs/php_codesniffer": "^3.5.0",
    "wp-coding-standards/wpcs": "^2.1.1"
  },
  "scripts": {
    "check-cs": [
      "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs"
    ],
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  }
}
